generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}


model Message {
  id                        String     @id @default(cuid())
  role                      String
  message                   String
  threadId                  String
  createdAt                 DateTime   @default(now())
}

