{
  "name": "ue-assistant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:local": "npx dotenv -e .env.local -- prisma generate && npx dotenv -e .env.local -- prisma migrate deploy && next build",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "npx dotenv -e .env.local -- npx prisma migrate dev --name",
    "primsa:create": "npx dotenv -e .env.local -- prisma migrate dev --create-only --name ",
    "prisma:generate": "npx dotenv -e .env.local -- prisma generate",
    "prisma:deploy": "npx dotenv -e .env.local -- prisma migrate deploy",
    "prisma:rollback": "npx dotenv -e .env.local -- npx prisma migrate resolve --rolled-back",
    "format": "prettier --write .",
    "postinstall": "prisma generate"
  },
